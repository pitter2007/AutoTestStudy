[
  {
    "description": "模拟一个没有参数的get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "text": "hello world"
    }
  },
  {
    "description": "模拟一个带参数的get请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries": {
        "name": "张三",
        "age": "18"
      }
    },
    "response": {
      "text": "are you 带参数的get 请求 again",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }
]